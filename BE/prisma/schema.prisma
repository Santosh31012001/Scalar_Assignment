generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model EventType {
  id          String   @id @default(uuid())
  name        String
  duration    Int
  slug        String   @unique
  description String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}

model Availability {
  id             String   @id @default(uuid())
  timezone       String   @default("UTC")
  weeklySchedule Json
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
}
